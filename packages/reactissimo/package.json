{
  "name": "reactissimo",
  "private": true,
  "version": "1.0.0",
  "main": "dist/reactissimo.umd.cjs",
  "module": "dist/reactissimo.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build:tsc": "npx tsc",
    "build:vite": "vite build",
    "build": "npm run build:vite && npm run build:tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "develop": "nodemon -e ts,tsx,css --watch ./src --exec \"npm run build\""
  },
  "directories": {
    "src": "src"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "@headlessui/react": "^2.0.0-alpha.4",
    "clsx": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "3.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
